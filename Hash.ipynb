        "import hashlib\n",
        "\n",
        "def main(): \n",
        "\n",
        "    text = \"harsh!\"\n",
        "    textUtf8 = text.encode(\"utf-8\")\n",
        "\n",
        "    hash = hashlib.md5( textUtf8 )\n",
        "    hexa = hash.hexdigest()\n",
        "\n",
        "    print ( hexa )\n",
        "\n",
        "    return\n",
        "\n",
        "main()"
      ],
